{
  "name": "construction-tracker",
  "version": "1.0.0",
  "description": "A comprehensive construction management application",
  "private": true,
  "scripts": {
    "start": "node server/server.js",
    "build": "npm install && cd server && npm install && cd ../client && npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "heroku-postbuild": "npm install && cd server && npm install && cd ../client && npm install && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manu-TechKid/-construction-tracker.git"
  },
  "keywords": [
    "construction",
    "management",
    "tracking",
    "react",
    "node",
    "mongodb"
  ],
  "author": "Manu-TechKid",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Manu-TechKid/-construction-tracker/issues"
  },
  "homepage": "https://github.com/Manu-TechKid/-construction-tracker#readme",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  }
}
